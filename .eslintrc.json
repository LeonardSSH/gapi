  "extends": ["plugin:@typescript-plugin-recommended", "prettier/@typescript-eslint", "plugin:prettier/recommended"],
  "rules": {
    "prefer-destructuring": "off",
    "@typescript-eslint/no-namespace": "off"
  }
}
